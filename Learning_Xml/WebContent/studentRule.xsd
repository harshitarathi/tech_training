   <xs:sequence>
     <xs:element name="name" type="xs:string"/>
     <xs:element name="age" type="xs:int"/>
      <xs:element name="dept" type="xs:string"/>
   </xs:sequence>
<xs:attribute name="title" type="xs:string"/>
</xs:complexType>

</xs:element>
     
</xs:schema>
